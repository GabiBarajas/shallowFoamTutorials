FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application shallowFoam;

//startFrom       latestTime;
startFrom       startTime;

startTime       0;

stopAt          endTime;
//stopAt          writeNow;

endTime         39600;

deltaT          0.02;

writeControl    adjustableRunTime;

writeInterval   300;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.2;

